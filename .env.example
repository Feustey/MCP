# API Keys
SPARKSEER_API_KEY=your_sparkseer_api_key
OPENAI_API_KEY=your_openai_api_key

# Redis Configuration
REDIS_URL=redis://localhost:6379
RESPONSE_CACHE_TTL=3600

# Environment
ENVIRONMENT=development

# RAG Configuration
RAG_DATA_PATH=./data
MODEL_NAME=gpt-3.5-turbo
EMBEDDING_MODEL=text-embedding-3-small

# Server Configuration
PORT=8000
HOST=0.0.0.0

# Rate Limiting Configuration
RATE_LIMIT_DEFAULT=60
RATE_LIMIT_OPTIMIZE=30
RATE_LIMIT_SPARKSEER=100
RATE_LIMIT_HEALTH=300
DAILY_QUOTA_DEFAULT=1000
DAILY_QUOTA_OPTIMIZE=500
DAILY_QUOTA_SPARKSEER=2000

# Request Optimization Configuration
BATCH_SIZE=10
MAX_CONCURRENT_REQUESTS=5
DEFAULT_PAGE_SIZE=10
MAX_PAGE_SIZE=100
REQUEST_TIMEOUT=30

# Authentication Configuration
JWT_SECRET_KEY=your_jwt_secret_key
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# User Roles Configuration
DEFAULT_USER_ROLE=user
ADMIN_USERNAME=admin
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=votre_mot_de_passe_admin 